generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model UserProfile {
  id        Int     @id @default(autoincrement())
  email     String  @unique
  name      String  @unique
  realName  String?
  picture   String?
}